{
  "name": "gl-bench",
  "version": "0.0.6",
  "description": "A proper WebGL performance benchmark that uses GPU timers inside",
  "main": "dist/gl-bench.cjs.js",
  "module": "dist/gl-bench.esm.js",
  "browser": "dist/gl-bench.js",
  "minimized": "dist/gl.bench.min.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "npm run build && npm-run-all -p watch serve open",
    "test": "node test.js --input-type=module",
    "watch": "rollup -c -w",
    "serve": "http-server -c-1 -p 1234",
    "open": "open-cli http://127.0.0.1:1234"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.10.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^5.0.0",
    "rollup": "^1.19.4"
  },
  "author": "munrocket",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/munrocket/gl-bench.git"
  },
  "keywords": [
    "webgl",
    "webgl2",
    "benchmark",
    "performance",
    "timer",
    "stats.js",
    "pixi",
    "three",
    "babylon",
    "playcanvas"
  ]
}
